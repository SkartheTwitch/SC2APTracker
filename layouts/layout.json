{
  "tracker": {
    "type": "container",
    "background": "#212121",
    "content": {
      "type": "dock",
      "dropshadow": true,
      "content": [
        {
          "type": "dock",
          "dock": "left",
          "margin": "0,0,3,0",
          "content": [
            {
              "type": "group",
              "header": "Items",
              "dock": "top",
              "header_content": {
                "type": "button_popup",
                "style": "settings",
                "popup_background": "#FF212121",
                "layout": "options_popup"
              },
              "content": {
                "type": "scroll",
                "horizontal_scrollbar_visibility": "disabled",
                "content": {
                  "type": "layout",
                  "key": "item_grid"
                }
              }
            }
          ]
        },
        {
          "type": "dock",
          "content": [
            {
              "type": "group",
              "header": "Pinned Locations",
              "dock": "left",
              "width": 200,
              "content": [
                {
                  "type": "scroll",
                  "horizontal_scrollbar_visibility": "disabled",
                  "content": {
                    "type": "recentpins",
                    "style": "wrap",
                    "h_alignment": "stretch",
                    "v_alignment": "stretch",
                    "orientation": "vertical",
                    "compact": true
                  }
                }
              ]
            },
            {
              "type": "tabbed",
              "tabs": [
                {
                  "title": "Wings of Liberty",
                  "icon": "images/factions/terran.png",
                  "content": {
                    "type": "map",
                    "maps": [
                      "wol"
                    ]
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  }
}